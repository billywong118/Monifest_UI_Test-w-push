<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.example" version="1.0.0" versionCode="1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="PushbotsPlugin">
        <param name="android-package" value="com.pushbots.plugin.PushbotsPlugin" />
    </feature>
    <name>Monifest</name>
    <description>
	Monifest app prototype!
	</description>
    <author email="" href="">
        author name
    </author>
    <preference name="permissions" value="none" />
    <gap:plugin name="com.phonegap.plugins.pushplugin" version="2.4.0" />
    <access origin="*" />
    <icon src="www/logo.png" />
</widget>
